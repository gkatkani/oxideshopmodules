{
    "name": "oxideshopmodules/g_attributemodel",
    "description": "Extended Filter",
    "type": "oxideshop-module",
    "keywords": ["extended filter", "search"],
    "license": "MIT",
    "authors": [
        {
            "name": "gaurav",
            "email": "gkatkani@gmail.com"
        }
    ],
    "minimum-stability": "dev", 
    "extra": {
        "oxideshop": {
            "blacklist-filter": [
                "documentation/**/*.*"
            ],
            "target-directory": "oxideshopmodules/g_attributemodel"
        }
    },
    "require": {
        "php": ">=5.6",
        "lib-curl": ">=7.26.0",
        "lib-openssl": ">=1.0.1",
        "ext-curl": "*",
        "ext-openssl": "*"
    },   
    
    "autoload-dev": {
        "psr-4": {
          "OxidEsales\\gkmegamenu\\": "../../../source/modules/oxideshopmodules/g_attributemodel/events/Gk_megamenu_events"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
